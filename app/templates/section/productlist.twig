<section id="list" class="container content-section text-center">
    <div class="row">
        <div class="col-lg-6" align="center">
            <h2>MaterialList <a href="#" data-toggle="modal" data-target="#modal-punch"><i class="fa fa-edit"></i></a></h2>
            <table class="table">
                <thead align="center">
                    <tr>
                        <th>Name</th>
                        <th>Material1</th>
                        <th>Material2</th>
                        <th>Material3</th>
                        <th>Material4</th>
                        <th>Material5</th>
                        <th>Material6</th>
                        <th>Material7</th>
                        <th>Material8</th>
                    </tr>
                </thead>
                <tbody>
                    {% for record in productrecords %}
                        <tr>
                            <td>{{ record.GoodsName }}</td>
                            <td>{{ record.Product1Needed }}</td>
                            <td>{{ record.Product2Needed }}</td>
                            <td>{{ record.Product3Needed }}</td>
                            <td>{{ record.Product4Needed }}</td>
                            <td>{{ record.Product5Needed }}</td>
                            <td>{{ record.Product6Needed }}</td>
                            <td>{{ record.Product7Needed }}</td>
                            <td>{{ record.Product8Needed }}</td>
                        </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</section>